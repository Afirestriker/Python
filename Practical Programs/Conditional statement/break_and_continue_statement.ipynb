    "# Break and continue statement uses\n",
    "\n",
    "print(\"Break statement use: \")\n",
    "n=1;\n",
    "while(n<=10):\n",
    "    if(n==4):\n",
    "        break\n",
    "    print(n)\n",
    "    n+=1\n",
    "\n",
    "print(\"Continue statement uses: \")\n",
    "n=1\n",
    "while(n<=10):\n",
    "    if(n==4):\n",
    "        n+=1\n",
    "        continue\n",
    "    print(n)\n",
    "    n+=1\n"
   ]
